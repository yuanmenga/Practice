{
  "name": "prettier-eslint",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:commit"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint:commit": "commitlint -e $GIT_PARAMS"
  },
  "dependencies": {
    "prettier": "^3.0.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
